node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def scannerHome = tool 'SonarScanner for MSBuild'
    withSonarQubeEnv() {
      bat "\"${scannerHome}\\dotnet\" \"${scannerHome}\\SonarScanner.MSBuild.dll\" begin /k:\".net-code\""
      bat "\"${scannerHome}\\dotnet\" build"
      bat "\"${scannerHome}\\dotnet\" \"${scannerHome}\\SonarScanner.MSBuild.dll\" end"
    }
  }
}
